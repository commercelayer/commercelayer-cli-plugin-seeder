{
  "name": "@commercelayer/cli-plugin-seeder",
  "description": "Commerce Layer CLI Seeder plugin",
  "version": "4.2.9",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-seeder/issues",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-seeder",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-seeder",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "seeder": {
        "description": "Execute Commerce Layer seeder",
        "hidden": true
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-seeder",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "build": "rm -rf lib && tsc -b",
    "prepack": "pnpm build && oclif manifest && pnpm readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --plugin --bin=commercelayer && git add README.md",
    "lint": "eslint src --ext .ts --config .eslintrc",
    "lint:fix": "eslint src --fix"
  },
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@commercelayer/cli-dev": "^0.1.10",
    "@commercelayer/eslint-config-ts": "^0.1.3",
    "@oclif/plugin-help": "^5.1.16",
    "@oclif/test": "^2.2.6",
    "@types/chai": "^4.3.3",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.7",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "pnpm": "^7.14.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@commercelayer/cli-core": "^1.6.1",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/sdk": "^4.16.0",
    "@oclif/core": "1.20.4",
    "axios": "^1.1.3",
    "listr": "^0.14.3",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
