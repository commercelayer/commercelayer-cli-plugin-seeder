{
  "name": "@commercelayer/cli-plugin-seeder",
  "description": "Commerce Layer CLI Seeder plugin",
  "version": "4.5.0",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-seeder/issues",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-seeder",
  "keywords": [
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-seeder",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "seeder": {
        "description": "Execute Commerce Layer seeder",
        "hidden": true
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-seeder",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "build": "rm -rf lib && tsc -b",
    "prepack": "pnpm build && oclif manifest && pnpm readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --plugin --bin=commercelayer && git add README.md",
    "lint": "eslint src --ext .ts --config .eslintrc",
    "lint:fix": "eslint src --fix"
  },
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@commercelayer/cli-dev": "^2.0.5",
    "@commercelayer/eslint-config-ts": "^1.0.0",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/test": "^2.3.21",
    "@types/chai": "^4.3.5",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@commercelayer/cli-core": "^3.7.3",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/sdk": "^5.2.3",
    "@oclif/core": "^2.8.5",
    "axios": "1.4.0",
    "listr": "^0.14.3",
    "tslib": "^2.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
